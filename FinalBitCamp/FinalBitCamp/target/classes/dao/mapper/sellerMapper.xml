<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.SellerDao">

<insert id="insertSeller" parameterType="seller">

	insert into seller values(
	#{businessNum}, #{businessName}, #{businessPhone}, #{businessAddr}, #{businessEmail}, #{businessPost}, #{sellerId}, #{joinDate}
	)

</insert>

<select id="selectOne" parameterType="String" resultType="seller">

	select * from seller where sellerId = #{sellerId}

</select>

</mapper>